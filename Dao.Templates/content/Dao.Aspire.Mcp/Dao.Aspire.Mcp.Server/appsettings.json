{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "{{insert tenant id}}",
    "ClientId": "{{insert client id}}",
    "Audience": "api://{{insert client id}}",
    "Scopes": [ "api://{{insert client id}}/{{insert scope here}}" ]
  }
}
